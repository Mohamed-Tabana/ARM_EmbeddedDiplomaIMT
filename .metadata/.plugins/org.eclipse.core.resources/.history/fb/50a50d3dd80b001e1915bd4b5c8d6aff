#ifndef MCAL_SPI_SPI_PRV_H_
#define MCAL_SPI_SPI_PRV_H_

typedef enum {
    CPHA = 0x00,
    CPOL = 0x01,
    MSTR = 0x02,
    BR0 = 0x03,
    SPE = 0x06,
    LSBFIRST,
    SSI,
    SSM,
    RXONLY,
    DFF,
    CRCNEXT,
    CRCEN,
    BIDIOE ,
    BIDIMODE
} SPI_CR1_Bits;

#endif /* MCAL_SPI_SPI_PRV_H_ */

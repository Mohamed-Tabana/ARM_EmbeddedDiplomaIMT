#ifndef MCAL_SPI_SPI_PRV_H_
#define MCAL_SPI_SPI_PRV_H_

typedef enum {
    SPI_CR1_CPHA = 0x00,
    SPI_CR1_CPOL = 0x01,
    SPI_CR1_MSTR = 0x02,
    SPI_CR1_BR0 = 0x03,
    SPI_CR1_SPE = 0x06,
    SPI_CR1_LSBFIRST,
    SPI_CR1_SSI,
    SPI_CR1_SSM,
    SPI_CR1_RXONLY,
    SPI_CR1_DFF,
    SPI_CR1_CRCNEXT,
    SPI_CR1_CRCEN,
    SPI_CR1_BIDIOE ,
    SPI_CR1_BIDIMODE
} SPI_CR1_Bits;

#endif /* MCAL_SPI_SPI_PRV_H_ */
